package work_1019_01;

public class work_06 {

	public static void main(String[] args) {
		
		int[][] a = new int [6][6];
		int sum = 0;
		for (int i = 0; i < a.length; i++) {
				
			for (int j = 0; j <= 5 - i; j++) {
				a[i][j] = ++sum;
				System.out.printf("%3d", a[i][j]);
			}
			System.out.println();
		}
	}

}
