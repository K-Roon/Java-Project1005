package work_1019_01;

public class work_05 {

	public static void main(String[] args) {
		
		int[][] a = new int [5][5];
		int sum = 0;
		for (int i = 0; i < 5; i++) {sum++;
			for (int j = 0; j < i; j++) {
				
				a[i][j] = sum;
				System.out.printf("%3d", i);
			}
			System.out.println();
		}
	}

}
