package work_1006_01;

import java.util.Scanner;

public class factoria {

	public static void main(String[] args) {
		Scanner scan = new Scanner(System.in);
		System.out.print("정수를 입력하세요: ");
		int n = scan.nextInt();
		int res =1;
		for (int i = 1; i<=n;i++) {
			res *= i;
			
		}
		System.out.println(res);
		
	}

}
